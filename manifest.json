{
    "manifest_version": 2,

    "name": "SRL Enhancer",
    "description": "Adds functionality to speedrunslive.com races",
    "version": "1.0",
    "content_scripts": [{
      "matches": ["http://www.speedrunslive.com/race/*","https://www.speedrunslive.com/race/*"],
      "css": ["moveIcons.css"],
      "js": ["jquery-2.1.3.min.js", "srlenhancer.js"]
    }],
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
